{
  "manifest_version": 2,

  "name": "BookMarks Development Build (React.js)",
  "description": "Organize your bookmarks like never before.",
  "version": "0.1.0",

  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "icons": {
     "16": "/Assets/Icons/logo16.png",
     "48": "/Assets/Icons/logo48.png",
     "128": "/Assets/Icons/logo128.png"
  },
  "browser_action": {
    "default_icon": "/Assets/Icons/logo16.png",
    "default_popup": "popup.html"
  },
  "incognito": "split",
  "background": {
    "scripts": ["/Assets/Scripts/background.js"]
   }
}